{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.2.2"
  },
  "scripts": {
    "finish": "hexo clean && hexo g && gulp && hexo d && git add . && git commit -m 'modify' && git push origin Hexo",
    "glups": "cnpm i -S gulp && cnpm i -S gulp-htmlclean && cnpm i -S  gulp-htmlmin && cnpm i -S gulp-imagemin && cnpm i -S gulp-minify-css && cnpm i -S gulp-uglify",
    "sitemap": "cnpm i hexo-generator-sitemap -S  &&  cnpm i hexo-generator-baidu-sitemap -S",
    "bd": "cnpm i hexo-baidu-url-submit -S",
    "un": "cnpm install hexo-pangu-spacing --save",
    "amd": "cnpm install --save amd-optimize",
    "ig": "cnpm install --save gulp-ignore"
  },
  "dependencies": {
    "amd-optimize": "^0.6.1",
    "gulp": "^3.9.1",
    "gulp-htmlclean": "^2.7.14",
    "gulp-htmlmin": "^3.0.0",
    "gulp-ignore": "^2.0.2",
    "gulp-imagemin": "^3.3.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^3.0.0",
    "hexo": "^3.2.0",
    "hexo-abbrlink": "^2.0.4",
    "hexo-autonofollow": "^1.0.1",
    "hexo-deployer-git": "^0.2.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-baidu-sitemap": "^0.1.2",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.0",
    "hexo-generator-index": "^0.2.1",
    "hexo-generator-search": "^1.0.3",
    "hexo-generator-sitemap": "^1.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-pangu-spacing": "^1.1.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "pangu": "^3.3.0",
    "qiniu": "^7.0.5"
  }
}
