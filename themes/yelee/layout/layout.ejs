<%- partial('_partial/head') %>
<body>
  <div id="container">
    <div class="left-col">
    <%- partial('_partial/left-col', null, {cache: !config.relative_link}) %>
    </div>
    <div class="mid-col">
      <% if(theme.particles.on) { %>
        <div id="particles-oli-wrapper">
          <canvas class="particles-js-canvas-el"></canvas>
        </div>
      <% } %>
      <%- partial('_partial/mobile-nav', null, {cache: !config.relative_link}) %>
      <div class="body-wrap"><%- body %></div>
      <% if(theme.photoswipe) { %>
        <%- partial('_partial/viewer') %>
      <% } %>
      <%- partial('_partial/footer') %>
    </div>
    <%- partial('_partial/after-footer') %>
  </div>
    <% if(theme.live2d.on) { %>
        <script type='text/javascript' src='<%- config.root %>js/live2d.js'></script>
        <canvas id="live2dcanvas" width="120" height="240" class="live2d"></canvas>
        <script>loadlive2d("live2dcanvas" ,"<%- config.root %>js/koharu.model.json")</script>
    <% } %>
</body>
</html>